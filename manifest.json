{
    "manifest_version": 2,
    "minimum_chrome_version": "20",
    "content_security_policy": "default-src 'self'",
    "default_locale": "en",
    "name": "__MSG_ext_name__",
    "description": "__MSG_ext_description__",
    "version": "0.5.5",
    "icons": {
        "128": "img/128.png",
         "48": "img/48.png",
         "16": "img/16.png"
    },
    "permissions": [
        "storage"
    ],
    "content_scripts": [{
        "matches": [
            "*://*.youtube.com/*"
        ],
        "css": [
            "YouTubeHTML5.css"
        ],
        "js": [
            "YouTubeHTML5.js"
        ],
        "run_at": "document_end"
    }],
    "web_accessible_resources": [
        "img/arrow.svg",
        "img/decrease.svg",
        "img/disabled.svg",
        "img/download.svg",
        "img/edge.svg",
        "img/increase.svg",
        "img/reload.svg",
        "img/speed.svg",
        "img/toggle.svg"
    ],
    "options_page": "options/options.html"
}
